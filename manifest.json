{
  "manifest_version": 3,
  "name": "Costly Browsing",
  "version": "1.0",
  "permissions": [
    "tabs",
    "webNavigation",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "assets/Coin.png"
    }
  }
}
